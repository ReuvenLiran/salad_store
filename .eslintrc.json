{
    "extends": "airbnb",
    "rules": {
        "react/forbid-prop-types": 1,
        "import/prefer-default-export": 0
    },
    "parser": "babel-eslint",
    "globals": {
        "describe": true,
        "test": true,
        "expect": true,
        "fetch": true,
        "navigator": true,
        "document": true,
        "window": true
    },
    "settings": {
      "import/core-modules": [
          "components", 
          "services", 
          "types", 
          "actions",
          "consts",
          "utils"
        ]
    }
}
